<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <!-- Add  -->
  <servlet>
  <servlet-name>add</servlet-name>
  <servlet-class>com.servlet.usiing.jdbc.MobileJdbc</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>display</servlet-name>
  <servlet-class>com.servlet.usiing.jdbc.DisplayMobile</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>add</servlet-name>
  <url-pattern>/addMobile</url-pattern>
  </servlet-mapping>
  
  <!-- Display -->
  
   
  <servlet-mapping>
  <servlet-name>display</servlet-name>
  <url-pattern>/DisplayMobile</url-pattern>
  </servlet-mapping>
  
</web-app>


